{
    "env": {
        "browser": true,
        "commonjs": true,
        "es6": true,
        "node": true
    },
    "extends": [
        "google"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018
    },
    "rules": {
        "linebreak-style": 0,
        "require-jsdoc": 0,
        "max-len": 0,
        "camelcase": 1,
        "no-console": 0,
        "prefer-template": 0,
        "global-require": 0
    }
}